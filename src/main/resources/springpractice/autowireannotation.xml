<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />

    <bean class ="org.spring.springpractice.autowireannotation.MarutiCar" name = "maruti">
        <property name="logo" value="MarutiLOGO"/>
    </bean>

    <bean class ="org.spring.springpractice.autowireannotation.MarutiCar" name = "maruti1">
        <property name="logo" value="MarutiLOGO1"/>
    </bean>

    <bean class ="org.spring.springpractice.autowireannotation.MarutiCar" name = "maruti12">
        <property name="logo" value="MarutiLOGO1"/>
    </bean>

    <bean class="org.spring.springpractice.autowireannotation.Car" name="car">
        <property name="color" value="red"/>
    </bean>
</beans>